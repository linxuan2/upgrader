<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping>
        <class-a>org.xlet.upgrader.domain.ChangeLog</class-a>
        <class-b>org.xlet.upgrader.vo.dashboard.ChangeLogDTO</class-b>
        <field>
            <a>version.version</a>
            <b>version</b>
        </field>
        <field>
            <a>version.product.id</a>
            <b>productId</b>
        </field>
        <field>
            <a>version.product.code</a>
            <b>product</b>
        </field>
        <field>
            <a>version.id</a>
            <b>versionId</b>
        </field>
        <field custom-converter-id="dateConverter">
            <a>updateAt</a>
            <b>updateAt</b>
        </field>
        <field>
            <a>image</a>
            <b>image</b>
        </field>
        <field custom-converter-id="dateConverter">
            <a>createAt</a>
            <b>createAt</b>
        </field>
    </mapping>

    <mapping>
        <class-a>org.xlet.upgrader.domain.ChangeLog</class-a>
        <class-b>org.xlet.upgrader.vo.ChangeLogVo</class-b>
        <field>
            <a>image</a>
            <b>image</b>
        </field>
    </mapping>

</mappings>